{
    "name": "deburca/www",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "defuse/php-encryption": "^2.2",
        "drupal/admin_toolbar": "^2.3",
        "drupal/advagg": "^4.0",
        "drupal/backup_migrate": "^5.0",
        "drupal/block_class": "^1.0",
        "drupal/bootstrap_barrio": "^5.0",
        "drupal/bootstrap_layouts": "^5.1",
        "drupal/bootstrap_library": "^1.11",
        "drupal/coder": "^8.3",
        "drupal/core": "^8.7.0 || ^9",
        "drupal/core-composer-scaffold": "^9.0.0",
        "drupal/core-recommended": "^9.0.0",
        "drupal/ctools": "^3.2",
        "drupal/devel": "^4.0",
        "drupal/ds": "^3.3",
        "drupal/entity_browser": "^2.2",
        "drupal/entity_reference_revisions": "^1.6",
        "drupal/field_formatter_class": "^1.1",
        "drupal/field_group": "^3.0",
        "drupal/geshifilter": "^2.0",
        "drupal/honeypot": "^2.0",
        "drupal/jquery_ui_accordion": "^1.1",
        "drupal/link_attributes": "^1.8",
        "drupal/metatag": "^1.13",
        "drupal/olivero": "^1.0",
        "drupal/pathauto": "^1.4",
        "drupal/redirect": "^1.4",
        "drupal/seckit": "^2.0",
        "drupal/views_bootstrap": "^4.1",
        "drupal/viewsreference": "^2.0",
        "drush/drush": "^10.0.0",
        "kint-php/kint": "^3.3",
        "phpstan/phpstan-strict-rules": "^0.12.2",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "drupal/core-dev": "^9.0.0",
        "edgedesign/phpqa": "^1.23",
        "friendsofphp/php-cs-fixer": "^2.16",
        "jakub-onderka/php-console-highlighter": "^0.4.0",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "mglaman/phpstan-drupal": "^0.12.3",
        "php-parallel-lint/php-console-color": "^0.3.0",
        "php-parallel-lint/php-console-highlighter": "^0.5.0",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "phpcompatibility/php-compatibility": "^9.3",
        "phpmd/phpmd": "@stable",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.19",
        "phpstan/phpstan-deprecation-rules": "^0.12.2",
        "rskuipers/php-assumptions": "^0.8.0",
        "sensiolabs/security-checker": "^6.0",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^3.11",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "locations": {
                "project-root": "./",
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/.htaccess": false
            },
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
